{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/server",
      "env": {},
      "args": [],
      "showLog": true,
      "preLaunchTask": "Build (Current Platform)"
    },
    {
      "name": "Launch Server (No Build)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/server",
      "env": {},
      "args": [],
      "showLog": true
    },
    {
      "name": "Debug Tests (Current File)",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${fileDirname}",
      "showLog": true
    },
    {
      "name": "Debug Tests (All)",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${workspaceFolder}/test",
      "showLog": true
    },
    {
      "name": "Attach to Process",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": "${command:pickProcess}"
    }
  ]
}
